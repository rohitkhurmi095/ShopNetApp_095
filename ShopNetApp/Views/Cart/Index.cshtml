@model List<ShopNetApp.Models.Product>

@{
    ViewData["Title"] = "Index";
}

<h1>Shopping Cart</h1>

@*-- Cart Products List *@
@foreach (var product in Model)
{
    <img src="@WC.ImagePath@product.Image" width="100" height="100" alt="@product.Name"/>
    <h6>@product.Id</h6>
    <h6>@product.Name</h6>
    <h6>@product.Price</h6>
    <hr />
    <br /><br />
}

